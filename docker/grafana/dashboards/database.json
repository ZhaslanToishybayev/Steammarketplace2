{
  "dashboard": {
    "id": null,
    "title": "Steam Marketplace - Database",
    "tags": ["steam", "marketplace", "database"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "PostgreSQL Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "pg_settings_max_connections",
            "legendFormat": "Max Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "PostgreSQL Query Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(pg_stat_statements_mean_time_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pg_stat_statements_mean_time_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pg_stat_statements_mean_time_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "PostgreSQL Slow Queries",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(pg_stat_statements_mean_time_sum[5m]) / rate(pg_stat_statements_calls[5m]))",
            "legendFormat": "Query",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "PostgreSQL Transaction Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(pg_stat_database_xact_commit[5m])",
            "legendFormat": "Commits/sec",
            "refId": "A"
          },
          {
            "expr": "rate(pg_stat_database_xact_rollback[5m])",
            "legendFormat": "Rollbacks/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Transactions/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "PostgreSQL Cache Hit Ratio",
        "type": "stat",
        "targets": [
          {
            "expr": "pg_stat_database_blks_hit / (pg_stat_database_blks_hit + pg_stat_database_blks_read) * 100",
            "legendFormat": "Cache Hit Ratio %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "PostgreSQL Deadlocks",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(pg_stat_database_deadlocks[5m])",
            "legendFormat": "Deadlocks/sec",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "MongoDB Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mongodb_op_counters_insert[5m])",
            "legendFormat": "Inserts/sec",
            "refId": "A"
          },
          {
            "expr": "rate(mongodb_op_counters_update[5m])",
            "legendFormat": "Updates/sec",
            "refId": "B"
          },
          {
            "expr": "rate(mongodb_op_counters_delete[5m])",
            "legendFormat": "Deletes/sec",
            "refId": "C"
          },
          {
            "expr": "rate(mongodb_op_counters_query[5m])",
            "legendFormat": "Queries/sec",
            "refId": "D"
          },
          {
            "expr": "rate(mongodb_op_counters_command[5m])",
            "legendFormat": "Commands/sec",
            "refId": "E"
          }
        ],
        "yAxes": [
          {
            "label": "Operations/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 8,
        "title": "MongoDB Query Execution Time",
        "type": "graph",
        "targets": [
          {
            "expr": "mongodb_op_counters_repl_total_execution_time / mongodb_op_counters_repl_total",
            "legendFormat": "Avg Execution Time",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "MongoDB Connection Pool Usage",
        "type": "stat",
        "targets": [
          {
            "expr": "mongodb_connections_current / mongodb_connections_max * 100",
            "legendFormat": "Connection Pool %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 10,
        "title": "Redis Commands/sec",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Commands/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 28
        }
      },
      {
        "id": 11,
        "title": "Redis Evicted Keys",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_evicted_keys_total[5m])",
            "legendFormat": "Evicted Keys/sec",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 28
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}